{
  "expo": {
    "name": "task-daily",
    "slug": "task-daily",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/to-do-list.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.manav.toDoApp",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.manav.toDoApp",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/to-do-list.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}